{
  "name": "razor-security",
  "version": "0.0.1",
  "scripts": {
    "build":"npm run build:server:authdal && npm run build:server:authhost && npm run build:server:authservice && npm run build:server:razorsecurity && npm run build:client",
    "build:server:authdal":"cd ./servers/authDal && npm run build",
    "build:server:authhost":"cd ./servers/authHost && npm run build",
